\"flex\";\n        } else {\n            imageRef.current.src = \"\";\n            imageRef.current.style.display = \"none\";\n        }\n\n        setImage(e.target.files[0]);\n    };\n\n    const handleSubmit = async (e) => {\n\n        console.log(\"sim\")\n        e.preventDefault();\n\n        const data = new FormData();\n\n\n        image && data.append(\"image\", image);\n        console.log(data);\n        setIsLoading(true);\n        const Student = getUser();\n        try {\n            await api.post(\"/students/\"+Student.studentId+\"/images\", data, {\n                headers: {\n                    \"Content-type\": \"multipart/form-data\"\n                }\n            });\n            alert('Imagem atualizada com sucesso!');\n\n            history.goBack();\n        } catch (error) {\n            console.error(error);\n            alert(error);\n        } finally{\n            setIsLoading(false);\n        }\n    }\n\n\n    return (\n        <Container>\n            <Header />\n            <FormNewPost onSubmit={handleSubmit}>\n                <h1>Nova Imagem</h1>\n\n                <input required  id='file' type=\"file\" accept=\"image/*\" onChange={handleImage}  />\n                <img alt=\"Pré-visualização\" ref={imageRef} />\n                <button>Enviar</button>\n            </FormNewPost>\n        </Container>\n    );\n}\n\nexport default NewImage;","/home/lucasgoianamaliciafaculdade/ReactFirst/fiap-social-web-job/src/pages/NewImage/style.js",[],"/home/lucasgoianamaliciafaculdade/ReactFirst/fiap-social-web-job/src/pages/Feed/styles.js",[],"/home/lucasgoianamaliciafaculdade/ReactFirst/fiap-social-web-job/src/pages/NewPost/styles.js",[],"/home/lucasgoianamaliciafaculdade/ReactFirst/fiap-social-web-job/src/components/Header/index.js",["201","202","203","204"],"import { Container, IconSignOut } from \"./styles\";\nimport imgLogo from \"../../assets/logo.png\";\nimport imgProfile from \"../../assets/profileStandard.png\";\nimport {getUser} from \"../../services/security\";\n\n\nfunction Header() {\n    const user = getUser();\n    return (\n        <Container>\n            <div>\n                <img src={imgLogo} id=\"logo\" />\n                <img src={getUser().image} />\n                <p>Fulano</p>\n                <a href={'./new-image'}>Alterar Foto de Perfil</a>\n            </div>\n            <IconSignOut />\n        </Container>\n    )\n}\n\nexport default Header;","/home/lucasgoianamaliciafaculdade/ReactFirst/fiap-social-web-job/src/components/Tag/index.js",[],"/home/lucasgoianamaliciafaculdade/ReactFirst/fiap-social-web-job/src/components/Post/index.js",["205","206","207"],"import { CardComent, CardPost } from \"./styles\";\nimport imgProfile from \"../../assets/profile.png\"\nimport { useState } from \"react\";\nimport { getUser } from \"../../services/security\";\nimport { format } from \"date-fns\";\nimport {add, api} from \"../../services/api\";\n\nfunction Post({ data }) {\n\n    let signedUser = getUser();\n\n    const [newComent, setComents]  = useState({\n        description: \"\",\n        disabledComment:true\n    });\n\n    const handleComents = (e) => {\n        setComents({disabledComment:disabledBtn(e)});\n        setComents(prev => ({...prev, [e.target.name]: e.target.value }));\n    }\n    const [showComents, setShowComents] = useState(false);\n\n    const toggleComents = () => setShowComents(!showComents);\n\n    function disabledBtn(e) {\n        if(e.target.value.length >= 10){\n            return false;\n        }\n        return true;\n    }\n\n    return (\n        <CardPost>\n            <header>\n                <img src={imgProfile} />\n                <div>\n                    <p>por {signedUser.studentId === data.Student.id ? \"você\" : data.Student.name}</p>\n                    <span>em {format(new Date(data.created_at), \"dd/MM/yyyy 'às' HH:mm\")}</span>\n                </div>\n            </header>\n            <main>\n                <div>\n                    <h2>{data.title}</h2>\n                    <p>{data.description}</p>\n                </div>\n                {data.image && <img src={data.image} alt=\"imagem do post\" />}\n                <footer>\n                    {data.Categories.map(c => <p>{c.description}</p>)}\n                </footer>\n            </main>\n            <footer>\n                <h3 onClick={toggleComents}>\n                    {\n                        data.Answers.length === 0 ?\n                            \"Seja o primeiro a comentar\" :\n                            `${data.Answers.length} Comentário${data.Answers.length > 1 && \"s\"}`\n                    }\n                </h3>\n                {showComents && (\n                    <>\n                        { data.Answers.map(newComent => <Coment newComent={newComent} />) }\n                    </>\n                )}\n                <div>\n                    <input placeholder=\"Comente este post\" name=\"coment\" onChange={handleComents}/>\n                    <button  disabled={newComent.disabledComment} onClick={() => add(data,newComent,setComents)}>Enviar</button>\n                </div>\n            </footer>\n        </CardPost>\n    );\n}\n\nfunction Coment({newComent}) {\n    return (\n        <CardComent>\n            <header>\n                <img src={imgProfile} />\n                <div>\n                    <p>por {newComent.Student.name}</p>\n                    <span>em {format(new Date(newComent.created_at), \"dd/MM/yyyy 'às' HH:mm\")}</span>\n                </div>\n            </header>\n            <p>{newComent.description}</p>\n        </CardComent>\n    );\n}\n\nexport default Post;","/home/lucasgoianamaliciafaculdade/ReactFirst/fiap-social-web-job/src/components/Input/index.js",[],"/home/lucasgoianamaliciafaculdade/ReactFirst/fiap-social-web-job/src/components/Select/index.js",[],"/home/lucasgoianamaliciafaculdade/ReactFirst/fiap-social-web-job/src/components/Header/styles.js",[],"/home/lucasgoia