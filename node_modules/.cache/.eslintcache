a={post} />)}\n            <FaButton onClick={handleNavigateToNewPost}>+</FaButton>\n        </Container>\n    );\n}\n\nexport default Feed;","/home/lucasgoianamaliciafaculdade/ReactFirst/fiap-social-web/src/pages/NewPost/index.js",[],"/home/lucasgoianamaliciafaculdade/ReactFirst/fiap-social-web/src/pages/NewPost/styles.js",[],"/home/lucasgoianamaliciafaculdade/ReactFirst/fiap-social-web/src/pages/Feed/styles.js",[],"/home/lucasgoianamaliciafaculdade/ReactFirst/fiap-social-web/src/components/Header/index.js",["187","188","189","190"],"/home/lucasgoianamaliciafaculdade/ReactFirst/fiap-social-web/src/components/Input/index.js",[],"/home/lucasgoianamaliciafaculdade/ReactFirst/fiap-social-web/src/components/Tag/index.js",[],"/home/lucasgoianamaliciafaculdade/ReactFirst/fiap-social-web/src/components/Post/index.js",["191","192","193"],"/home/lucasgoianamaliciafaculdade/ReactFirst/fiap-social-web/src/components/Select/index.js",[],"/home/lucasgoianamaliciafaculdade/ReactFirst/fiap-social-web/src/components/Header/styles.js",[],"/home/lucasgoianamaliciafaculdade/ReactFirst/fiap-social-web/src/components/Input/styles.js",[],"/home/lucasgoianamaliciafaculdade/ReactFirst/fiap-social-web/src/components/Tag/styles.js",[],"/home/lucasgoianamaliciafaculdade/ReactFirst/fiap-social-web/src/components/Post/styles.js",[],"/home/lucasgoianamaliciafaculdade/ReactFirst/fiap-social-web/src/components/Select/styles.js",[],"/home/lucasgoianamaliciafaculdade/ReactFirst/fiap-social-web/src/pages/NewImage/index.js",["194","195","196"],"/home/lucasgoianamaliciafaculdade/ReactFirst/fiap-social-web/src/pages/NewImage/style.js",[],"/home/lucasgoianamaliciafaculdade/ReactFirst/fiap-social-web-job/src/index.js",[],"/home/lucasgoianamaliciafaculdade/ReactFirst/fiap-social-web-job/src/App.js",[],"/home/lucasgoianamaliciafaculdade/ReactFirst/fiap-social-web-job/src/GlobalStyles.js",[],"/home/lucasgoianamaliciafaculdade/ReactFirst/fiap-social-web-job/src/routes.js",[],"/home/lucasgoianamaliciafaculdade